#include<iostream>
#include<string>
using namespace std;
string max (string la, string lb, string lc) {
int length_a=la.length();
int length_b=lb.length();
int length_c=lc.length();
if(la>lb&&la>lc){
return a;
}else if(lb>la&&lb>lc) {
return b;
}else if(lc>la&&lc>lb) {
return c;
}
}
int main() 
{
string maxs;
string f="apple", g="banana", h="pineapple "
maxs=max(f, g, h);
cout << "maximum is:" <<maxs<< endl;
return 0;
}
